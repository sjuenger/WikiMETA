SELECT DISTINCT ?var1  ?var1Label  ?var2 
WHERE {
  ?var1  <http://www.wikidata.org/prop/direct/P1462>  <http://www.wikidata.org/entity/Q37033> .
 OPTIONAL {
  ?var1  <http://www.wikidata.org/prop/direct/P856>  ?var2 .
 }
 SERVICE  <http://wikiba.se/ontology#label>   {
    <http://www.bigdata.com/rdf#serviceParam>  <http://wikiba.se/ontology#language>  "en".
  }
}
ORDER BY ASC( ?var1Label )
